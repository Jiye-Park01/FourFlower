<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAEAEA"
    tools:context=".Dep3_CustomerService">

    <View
        android:id="@+id/Action_bar_sd"
        android:layout_width="match_parent"
        android:layout_height="75dp"

        android:background="@drawable/h_button_round"
        android:backgroundTint="#BCBBBB"
        android:fontFamily="@font/suitemedium"
        />

    <View
        android:id="@+id/Action_bar"
        android:layout_width="match_parent"
        android:layout_height="70dp"

        android:background="@drawable/h_button_round"
        android:backgroundTint="#758652"
        android:fontFamily="@font/suitemedium"

        android:textColor="#FFFFFF"
        android:textSize="24dp" />

    <ImageView
        android:id="@+id/back"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/j_backbutton"
        android:backgroundTint="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/suitemedium"
        android:text="FAQ"
        android:textColor="#FFFFFF"
        android:textSize="30dp" />

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Action_bar_sd"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="40dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="자주 묻는 질문"
                android:textColor="#000000"
                android:textSize="20dp"
                android:fontFamily="@font/suitemedium" />
        </LinearLayout>


        <LinearLayout
            android:id="@+id/distrupt_time"
            android:layout_width="328dp"
            android:layout_height="50dp"
            android:background="@drawable/j_round_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_centerVertical="true"
                android:text="비밀번호 변경"
                android:textSize="20dp"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/suitemedium"
                android:textColor="#000000" />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />
            <Button
                android:id="@+id/distrupt_timeButton"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/j_live_area"
                android:backgroundTint="#BCBBBB" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/distruptTime_2ut"
            android:layout_width="328dp"
            android:layout_height="50dp"
            android:background="@drawable/j_round_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_centerVertical="true"
                android:text="본인인증"
                android:textSize="20dp"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/suitemedium"
                android:textColor="#000000" />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />
            <Button
                android:id="@+id/distruptTime_2utButton"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/j_live_area"
                android:backgroundTint="#BCBBBB" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/on1_1"
            android:layout_width="328dp"
            android:layout_height="50dp"
            android:background="@drawable/j_round_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_centerVertical="true"
                android:text="주소지 변경"
                android:textSize="20dp"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/suitemedium"
                android:textColor="#000000" />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />
            <Button
                android:id="@+id/on1_1_Button"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/j_live_area"
                android:backgroundTint="#BCBBBB" />


        </LinearLayout>




    </LinearLayout>



</RelativeLayout>